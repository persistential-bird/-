#define  _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>

int main()//数据的组织形式
{
	int a = 10000;
	FILE* pf= fopen("test.txt ","wb");
	fwrite(&a, 4, 1, pf);//二进制的形式写到文件中
	fclose(pf);
	pf = NULL;

	return 0;
}

struct _iobuf//FILE结构体
{
	char *_ptr;
    int _cnt;
	char *_base;
    int fIag;
    int file;
	int _charbuf;
    int _bufsiz;
    char *_tmpfname;
};
typedef struct _iobuf FILE;
